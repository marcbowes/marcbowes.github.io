---
layout: nil
---

<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Marc Bowes</title>
  <link href="http://marc-bowes.com.com/"/>
  <link type="application/atom+xml" rel="self" href="http://marc-bowes.com/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://marc-bowes.com/</id>
  <author>
    <name>Marc Bowes</name>
    <email>marcbowes+blog@gmail.com</email>
  </author>

  {% for post in site.posts %}
  <entry>
    <id>http://marc-bowes.com{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="http://marc-bowes.com{{ post.url }}"/>
    <title>{{ post.title }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Marc Bowes</name>
      <uri>http://marc-bowes.com/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
 
</feed>
